generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider  = "postgresql"
  url       = env("POSTGRES_PRISMA_URL") // uses connection pooling
  directUrl = env("POSTGRES_URL_NON_POOLING") // uses a direct connection
}

model test {
  id   String @unique @db.VarChar(255)
  name String
}

model Menu {
  id    String   @id @default(cuid())
  name  String
  date  DateTime
  lunch Boolean
}
